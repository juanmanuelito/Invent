<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="inventarioBundle\Entity\Tgeneraproduct" table="TGeneraProduct">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomproduct" type="string" column="nomProduct" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="desproduct" type="string" column="desProduct" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usrcreate" type="string" column="usrCreate" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="feccreaci" type="datetime" column="fecCreaci" nullable="false"/>
    <field name="usrmodify" type="string" column="usrModify" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecmodify" type="datetime" column="fecModify" nullable="true"/>
  </entity>
</doctrine-mapping>
